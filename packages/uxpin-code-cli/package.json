{
  "name": "uxpin-code",
  "version": "1.0.0",
  "description": "Command-line tool integrating Design System repository with http://code.uxpin.com",
  "main": "dist/index.js",
  "bin": "bin/uxpin-code",
  "author": "UXPin Inc.",
  "license": "MIT",
  "keywords": [
    "UXPin",
    "design system",
    "command-line",
    "style guide",
    "UI components"
  ],
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@shopify/polaris": "https://github.com/Shopify/polaris.git",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "arui-feather": "https://github.com/alfa-laboratory/arui-feather.git",
    "child-process-promise": "^2.2.1",
    "codecov": "^2.3.0",
    "jest": "^21.0.1",
    "jest-junit-reporter": "^1.1.0",
    "nordnet-ui-kit": "https://github.com/nordnet/nordnet-ui-kit.git",
    "nyc": "^11.2.1",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "tslint-config-uxpin": "latest",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "fs-readfile-promise": "^3.0.0",
    "lodash": "^4.17.4",
    "react-docgen": "^2.18.0",
    "react-docgen-typescript": "^1.0.2"
  }
}
