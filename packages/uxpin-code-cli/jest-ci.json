{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "coverageReporters": [
    "json"
  ],
  "testMatch": [
    "**/test/**/*.test.js"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/test/resources/",
    "<rootDir>/test/integration/server/"
  ],
  "testEnvironment": "node",
  "testResultsProcessor": "jest-junit-reporter",
  "maxWorkers": 1
}
